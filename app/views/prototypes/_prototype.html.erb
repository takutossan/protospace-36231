<div class="card">
  <%# <%= image_tag prototype.image, class: 'prototype-image' if prototype.image.attached? %> %>
  <%= link_to image_tag(prototype.image, class: :card__img ), prototype_path(prototype.id) if prototype.image.attached? %>
  <div class="card__body">
    <%= prototype.title %>
    <p class="card__summary">
      <%= prototype.catch_copy %>
    </p>
    <%= link_to "by #{prototype.user.name}", root_path, class: :card__user %>
  </div>
</div>

